<zk>
	<window id="modifDialog" title="Modification d'une demande"
		mode="modal" closable="true" action="show: slideDown;hide: slideUp"
		apply="org.zkoss.bind.BindComposer"
		viewModel="@id('vm') @init('nc.noumea.mairie.kiosque.viewModel.ImprimerDemandesAgentViewModel')">
		<style>.fond { background-color:#FFEBCD; }</style>
		<div sclass="fond" height="100%">
			<iframe height="100%" width="100%" id="reportframe"
				content="@bind(vm.fileContent)">
			</iframe>
		</div>
	</window>
</zk>