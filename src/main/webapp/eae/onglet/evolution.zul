						<radio label="Non"
							disabled="@bind(not vm.modification)" value="${false}" />
					</radiogroup>
				</div>
			</hlayout>
			<hlayout>
				<div sclass="espace">
					<label value="Changement de métier" />
				</div>
				<div sclass="bleu">
